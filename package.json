{
  "name": "@logicalec/erp-api",
  "version": "1.0.0",
  "private": true,
  "description": "A REST API for @logicalec/erp-ui dashboard",
  "keywords": [
    "LGEC",
    "logicalec",
    "user",
    "panel",
    "api",
    "koa"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "UNLICENSED",
  "author": "Pablo Arredondo <parredondo@dlogical.ec> (https://logical.ec/)",
  "main": "app.js",
  "files": [
    "app.js",
    "src/",
    "!src/**/*.test.js"
  ],
  "scripts": {
    "db:create": "node ./knex/knex-manage.js create --env mysql",
    "db:destroy": "node ./knex/knex-manage.js destroy --env mysql",
    "db:list": "knex migrate:list --env mysql",
    "db:reset": "knex migrate:rollback --all --env mysql",
    "db:rollback": "knex migrate:rollback --env mysql",
    "db:seed": "knex seed:run --env mysql",
    "db:setup": "run-s db:create db:sync db:seed",
    "db:sync": "knex migrate:latest --env mysql",
    "debug": "node inspect app",
    "dev": "node --no-warnings app --auth.jwt=false",
    "lint": "eslint src/ app.js",
    "nodemon": "supervisor --watch src/ --watch ./app.js --exec yarn dev",
    "prettier": "prettier --config .prettierrc.json --write \"src/**/*.js\" \"knex/**/*.js\" *.js",
    "start": "node --no-warnings app",
    "test": "jest --env=node --colors --bail --detectOpenHandles --errorOnDeprecated --noStackTrace src/",
    "validate": "run-s lint 'test -- --coverage'"
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@koa/multer": "^3.0.0",
    "@koa/router": "^9.3.1",
    "@types/nunjucks": "^3.1.3",
    "async-lock": "^1.2.4",
    "csv-parse": "^4.11.1",
    "deep-freeze-strict": "^1.1.1",
    "escape-string-regexp": "^4.0.0",
    "got": "^11.5.1",
    "initials": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.2",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-compress": "^5.0.1",
    "koa-helmet": "^5.2.0",
    "koa-json": "^2.0.2",
    "koa-json-error": "^3.1.2",
    "koa-jwt": "^4.0.0",
    "koa-morgan": "^1.0.1",
    "koa-response-time": "^2.1.0",
    "micromatch": "^4.0.2",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "nconf": "^0.11.3",
    "nconf-yaml": "^1.0.2",
    "nunjucks": "^3.2.2",
    "p-retry": "^4.2.0",
    "ramda": "^0.27.1",
    "secure-random-password": "^0.2.1",
    "winston": "^3.3.3",
    "yargs-parser": "^18.1.3",
    "yn": "^4.0.0",
    "yup": "^0.29.3",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/jest": "^26.0.9",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/koa": "^2.11.3",
    "@types/koa-router": "^7.4.1",
    "@types/ms": "^0.7.31",
    "@types/mysql": "^2.15.15",
    "@types/nconf": "^0.10.0",
    "@types/ramda": "^0.27.14",
    "@types/yup": "^0.29.4",
    "chalk": "^4.1.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-ramda": "^2.5.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-sort-destructure-keys": "^1.3.5",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "jest": "^26.2.2",
    "lint-staged": "^10.2.11",
    "log-symbols": "^4.0.0",
    "log-update": "^4.0.0",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "prompt": "^1.0.0",
    "yargs": "^15.4.1"
  },
  "engines": {
    "node": ">= 10.0.0"
  }
}
